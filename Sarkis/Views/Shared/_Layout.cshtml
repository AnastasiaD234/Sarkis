@using System.Web.Optimization

<!DOCTYPE html>
<html lang="en">

<head>
     <meta charset="utf-8">
     <meta content="width=device-width, initial-scale=1.0" name="viewport">

     <title>La Sarkis Rzestaurant - Index| @ViewBag.Title</title>
